<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GroupSchedulesPage">

    <TextView
        android:id="@+id/bottomNav2"
        android:layout_width="412sp"
        android:layout_height="85sp"
        android:background="@color/timecrab_blue"
        android:fontFamily="@font/acme"
        android:paddingLeft="15sp"
        android:paddingTop="10sp"
        android:text=""
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/homeButton"
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:layout_marginEnd="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/bottomNav2"
        app:layout_constraintEnd_toStartOf="@+id/calendarButton"
        app:layout_constraintTop_toTopOf="@+id/bottomNav2"
        app:srcCompat="@drawable/home"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/calendarButton"
        android:layout_width="78sp"
        android:layout_height="70sp"
        android:layout_marginEnd="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/bottomNav2"
        app:layout_constraintEnd_toStartOf="@+id/profileButton"
        app:layout_constraintTop_toTopOf="@+id/bottomNav2"
        app:srcCompat="@drawable/calendar" />

    <ImageButton
        android:id="@+id/profileButton"
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:layout_marginEnd="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/bottomNav2"
        app:layout_constraintTop_toTopOf="@+id/bottomNav2"
        app:srcCompat="@drawable/samplepfp" />

    <ImageButton
        android:id="@+id/groupsButton"
        android:layout_width="70sp"
        android:layout_height="76sp"
        android:layout_marginStart="8sp"
        app:layout_constraintBottom_toBottomOf="@+id/bottomNav2"
        app:layout_constraintEnd_toEndOf="@+id/bottomNav2"
        app:layout_constraintHorizontal_bias="0.023"
        app:layout_constraintStart_toStartOf="@+id/bottomNav2"
        app:layout_constraintTop_toTopOf="@+id/bottomNav2"
        app:srcCompat="@drawable/groups" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/groupsched" />


</androidx.constraintlayout.widget.ConstraintLayout>